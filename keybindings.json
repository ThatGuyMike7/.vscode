// Place your key bindings in this file to override the defaults
[
    // "F6" to build the project.
    {
        "key": "f6",
        "command": "workbench.action.tasks.runTask",
        "args": "build x64 debug"
    },

    // Disable fullscreen toggle.
    { "key": "f11", "command": "" },

    // Disable zoom key bindings.
    { "key": "ctrl+numpad_add",       "command": "" },
    { "key": "ctrl+shift+oem_plus",   "command": "" },
    { "key": "ctrl+oem_plus",         "command": "" },
    { "key": "ctrl+numpad_subtract",  "command": "" },
    { "key": "ctrl+shift+oem_minus",  "command": "" },
    { "key": "ctrl+oem_minus",        "command": "" },
    { "key": "ctrl+numpad0",          "command": "" }
]